{
	"configuration": 
	{
		"status": "error",
		"name": "LinkLander",
		"packages": "de.lander",

		"appenders": 
		{
			"Console": 
			{
				"name": "STDOUT",
				"PatternLayout": 
				{
					"pattern": "%-5p [%t]: %m%n"
				}
			}
		},

		"loggers": 
		{
			"logger": 
			[
				{
					"name": "de.lander",
					"level": "trace",
					"additivity": "false",
					"AppenderRef": 
					{
						"ref": "STDOUT"
					}
				}
			],

			"root": 
			{
				"level": "error",
				"AppenderRef": 
				{
					"ref": "STDOUT"
				}
			}
		}
	}
}